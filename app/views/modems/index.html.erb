<h1>Listing modems</h1>

<table>
  <tr>
    <th>Model</th>
    <th>Modem type</th>
    <th>Speed</th>
    <th>Source</th>
    <th>Commissioning</th>
    <th>Task</th>
    <th>Decommissioning</th>
    <th>Comment</th>
    <th>Station</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @modems.each do |modem| %>
  <tr>
    <td><%= modem.model %></td>
    <td><%= modem.modem_type %></td>
    <td><%= modem.speed %></td>
    <td><%= modem.source %></td>
    <td><%= modem.commissioning %></td>
    <td><%= modem.task %></td>
    <td><%= modem.decommissioning %></td>
    <td><%= modem.comment %></td>
    <td><%= modem.station_id %></td>
    <td><%= link_to 'Show', station_modems_path %></td>
   <td><%= link_to 'Edit', '#' %></td>
    <td><%= link_to 'Destroy', '#' %></td>
  </tr>
<% end %>
</table>

